{
    "extends": "standard",
    "globals": {
        "$": false
    },
    "rules": {
        "semi": ["error", "always"],
        "space-before-function-paren": ["error", {"anonymous": "always", "named": "never"}],
        "no-unused-vars": ["error", { "varsIgnorePattern": "^menuBarToggler$|^messageBarHider$|^gameMenuToggler$" }]
    }
}